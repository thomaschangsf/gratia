# Pt1: Given a a lyrics (seq of string), return True if the word appears in
		# O() --> O(N)

def is_word_in_lyric(lyrics, word):
	j = 0
	for one_word_in_lyric in lyrics.split(" "):
		if word[j] in one_word_in_lyric:
			j+=1
		if j == len(word):
			break
	return len(word) == j


# Pt2: Given a window
		NO: return i_end - i_start < window if i_start and i_end
			if 0 returns False

			return i_end - i_start < window if i_start is not None and i_end is not None

	def is_word_in_lyric(lyrics, word):
		i_start, i_end = None, Noe
		j = 0
		N = len(word)
		for i, one_word_in_lyric in enumerate(lyrics.split(" ")):
			if word[j] in one_word_in_lyric:
				if i == 0:
					i_start = i
				j+=1

			if j == len(word):
				i_end = i
				break

		return j == N if (i_start is not None and i_end is not None) else False

		if 0 --> False



